{
  "expo": {
    "name": "LangMaster",
    "slug": "LangMaster",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./app/images/adaptive-icon.png",
        "backgroundColor": "#000000"
      },
      "package": "com.mkrlangmaster.LangMaster"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./app/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./app/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#000000"
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "a9990d10-035c-43cf-9e2a-1ae07b19723d"
      }
    },
    "owner": "mkr-langmaster"
  }
}
